<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.otaino2.ass2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="se.otaino2.ass2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="se.otaino2.ass2.gallery.GalleryActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="se.otaino2.ass2.contacts.ContactsPickerActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="se.otaino2.ass2.contacts.TaggedContactsActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>